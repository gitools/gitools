#mvn install:install-file -Dfile=ext/cobra-no-commons.jar -DgroupId=cobra -DartifactId=cobra-no-commons -Dversion=0.98.4 -Dpackaging=jar -DgeneratePom=true

# cobra html renderer
mvn install:install-file -Dfile=lib/cobra.jar -DgroupId=cobra -DartifactId=cobra -Dversion=0.98.4 -Dpackaging=jar -DgeneratePom=true

# weka machine learning tool
mvn install:install-file -Dfile=lib/weka.jar -DgroupId=weka -DartifactId=weka -Dversion=3.6.3 -Dpackaging=jar -DgeneratePom=true
