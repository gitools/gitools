#!/bin/bash

# commons-csv
mvn install:install-file -Dfile=lib/commons-csv-20070823.jar -DgroupId=commons-csv -DartifactId=commons-csv -Dversion=20070823 -Dpackaging=jar -DgeneratePom=true

# lobo browser http://lobobrowser.org/index.jsp
