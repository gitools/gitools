<?xml version="1.0" encoding="iso-8859-1"?>
<report>
	<title>Testing zcalc-report with TFBS-transfac modules</title>
	<laboratory>Biomedical Genomics Lab</laboratory>
	<url>http://bg.imim.es</url>
	
	<properties>
		<property name="Author" value="Christian" />
		<property name="Date" value="Jul 31, 2008" />
	</properties>
	
	<analyses>
		<analysis path="pru/zscore-mean" title="Zscore using mean estimator">
	
			<properties>
				<property name="Notes" value="Only modules greater than 10000 are showed" />
			</properties>
	
			<views>
				<view name="colorscale-table" title="P-Values with logarithmic color scale">
					<param>right-p-value</param>
					<scale>log</scale>
					<min-module-size>10000</min-module-size>
					<sort>
						<ascending>true</ascending>
						<condition>0</condition>
						<param>right-p-value</param>
					</sort>
				</view>
			</views>
		</analysis>
		
		<analysis path="pru/zscore-median" title="Zscore using median estimator">
	
			<properties>
				<property name="Notes" value="Only modules greater than 10000 are showed" />
