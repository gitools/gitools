					<sort>
						<ascending>true</ascending>
						<condition>0</condition>
						<param>right-p-value</param>
					</sort>
				</view>
			</views>
		</analysis>
		
		<analysis path="pru/binomial-poisson" title="Binomial with Poisson aproximation">
	
			<properties>
				<property name="Notes" value="Only modules greater than 10000 are showed" />
			</properties>
	
			<views>
				<view name="colorscale-table" title="P-Values with logarithmic color scale">
